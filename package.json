{
  "name": "tor-user-stats",
  "version": "0.4.1",
  "description": "Inofficial unser stats for /r/TranscribersOfReddit",
  "main": "dist/src/main.js",
  "author": "Tim Jentzsch",
  "license": "MIT",
  "scripts": {
    "init-config": "cp -n config/reddit.example.config.json config/reddit.config.json",
    "build": "yarn init-config && yarn tsc --build",
    "start": "yarn init-config && yarn build && node dist/src/main.js",
    "bundle": "yarn init-config && cross-env NODE_ENV=production parcel build templates/* --public-url .",
    "serve": "yarn init-config && parcel serve templates/*.html",
    "deploy": "gh-pages -d dist",
    "lint": "yarn init-config && eslint \"src/**/*\"",
    "format": "yarn init-config && eslint \"src/**/*\" --fix",
    "test": "yarn init-config && jest --config jest.config.json",
    "coverage": "yarn init-config && jest --config jest.config.json --coverage"
  },
  "engines": {
    "node": ">=14.15.1",
    "yarn": ">=1.22.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "env FORCE_COLOR=1 yarn lint",
      "pre-push": "env FORCE_COLOR=1 yarn build"
    }
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.9",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "cross-env": "7.0.2",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.1.4",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "parcel-bundler": "1.12.4",
    "prettier": "2.2.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/plotly.js": "^1.54.2",
    "escape-string-regexp": "4.0.0",
    "plotly.js-dist": "^1.57.1",
    "snoowrap": "1.21.0",
    "winston": "3.3.3"
  }
}
